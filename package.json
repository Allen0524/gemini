{
  "private": true,
  "name": "gemini",
  "version": "0.0.0",
  "main": "index.js",
  "license": "MIT",
  "workspaces": [
    "packages/*/*"
  ],
  "scripts": {
    "storybook": "storybook dev -p 6006",
    "dev": "yarn storybook",
    "build": "parcel build 'packages/*/*' --no-cache",
    "publish:stable": "yarn clean && yarn build && yarn workspaces foreach -pv --exclude gemini npm publish --tolerate-republish --access public",
    "clean": "yarn workspaces foreach -pv --exclude gemini run clean",
    "build-storybook": "storybook build"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.21.5",
    "@babel/plugin-transform-typescript": "^7.21.3",
    "@babel/preset-react": "^7.18.6",
    "@parcel/babel-plugin-transform-runtime": "^2.8.3",
    "@parcel/babel-preset-env": "^2.8.3",
    "@parcel/packager-ts": "2.8.3",
    "@parcel/transformer-typescript-types": "2.8.3",
    "@storybook/addon-essentials": "^7.0.9",
    "@storybook/addon-interactions": "^7.0.9",
    "@storybook/addon-links": "^7.0.9",
    "@storybook/addon-storysource": "^7.0.7",
    "@storybook/blocks": "^7.0.9",
    "@storybook/react": "^7.0.9",
    "@storybook/react-webpack5": "^7.0.9",
    "@storybook/testing-library": "^0.0.14-next.2",
    "@types/eslint": "^8.37.0",
    "@types/jest": "^29.5.1",
    "@types/jest-axe": "^3.5.5",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.1",
    "@typescript-eslint/eslint-plugin": "^5.59.1",
    "@typescript-eslint/parser": "^5.59.1",
    "babel-loader": "^9.1.2",
    "eslint": "^8.39.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "jest": "^29.5.0",
    "jest-axe": "^7.0.1",
    "parcel": "^2.8.3",
    "prettier": "^2.8.8",
    "prop-types": "^15.8.1",
    "storybook": "^7.0.9",
    "typescript": "^5.0.4"
  },
  "prettier": {
    "printWidth": 100,
    "singleQuote": true
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "plugin:jsx-a11y/recommended"
    ],
    "plugins": [
      "jsx-a11y"
    ]
  },
  "eslintIgnore": [
    "node_modules",
    "dist"
  ]
}
